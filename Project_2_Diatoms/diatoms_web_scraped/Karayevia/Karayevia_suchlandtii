<script>$(document).ready(function(){
  let revealOverlay = document.querySelector('.reveal-overlay');
  var globalModal = document.querySelector('#globalModal');
  
  if (globalModal) {
    if (window.sessionStorage.getItem('close-banner') === '1') {
      $('#globalModal').foundation('close');
    } else {
      $('#globalModal').foundation('open');
      
      globalModal.addEventListener('click', function (e) {
        window.sessionStorage.setItem('close-banner', '1');
      });
    }
  }
});</script>

